<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ccbdbdbd" />
            <corners android:radius="@dimen/cornerRadius5dp" />
        </shape>
    </item>
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/cornerRadius5dp" />
            <solid android:color="#f5f5f5" />
            <stroke
                android:width="1dp"
                android:color="#f5f5f5" />
        </shape>
    </item>
</layer-list>
