<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:backgroundTint="@color/mainTheme"
    android:paddingLeft="20dp"
    android:tag="enable">

    <FrameLayout
        android:id="@+id/boboboxPlaceHolderHotelHomeFL"
        android:layout_width="260dp"
        android:layout_height="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:elevation="@dimen/elevation4dp"
        app:layout_constraintDimensionRatio="H,13:6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/round_corner_black_transparent_10dp"
        app:layout_constraintBottom_toBottomOf="@+id/boboboxPlaceHolderHotelHomeFL"
        app:layout_constraintLeft_toLeftOf="@+id/boboboxPlaceHolderHotelHomeFL"
        app:layout_constraintRight_toRightOf="@+id/boboboxPlaceHolderHotelHomeFL"
        app:layout_constraintTop_toTopOf="@+id/boboboxPlaceHolderHotelHomeFL" />
</androidx.constraintlayout.widget.ConstraintLayout>
